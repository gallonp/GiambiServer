<?xml version="1.0" encoding="utf-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns="http://java.sun.com/xml/ns/javaee"
xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
	<servlet>
		<servlet-name>GiambiServer</servlet-name>
		<servlet-class>com.example.giambiserver.GiambiServerServlet</servlet-class>
	</servlet>
	<servlet>
	   	<servlet-name>RegisterLoginServer</servlet-name>
		<servlet-class>com.example.giambiserver.RegisterAndLoginServerlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GiambiServer</servlet-name>
		<url-pattern>/giambiserver</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
	    <servlet-name>RegisterLoginServer</servlet-name>
		<url-pattern>/registerLogin</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>
